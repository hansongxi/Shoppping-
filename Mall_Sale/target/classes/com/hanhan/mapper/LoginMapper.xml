<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hanhan.mapper.LoginMapper">
	<!--select_login  -->
	<select id="select_login" parameterType="com.hanhan.bean.T_MALL_USER_ACCOUNT"
		resultType="com.hanhan.bean.T_MALL_USER_ACCOUNT" 
		>
			select * from T_MALL_USER_ACCOUNT 
			where yh_mch = #{yh_mch} and
			yh_mm = #{yh_mm}
	</select>
</mapper>
